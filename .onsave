VAR:
	# variable
ENV:
	# environment variable
CONSOLE : right

LISTENER:
	-	CMD : echo "Hello $FILE"
		CONSOLE : right
		WATCH : *.coffee
		EXCLUDE: test.js
		TIMEOUT : 5000

	-	CMD : echo "Hello $FILE again"

#  $ROOT       = .onsave file directory
#  $FILE       = current file path
#  $FILENAME   = current file name
#  $BASENAME   = current file name, not postfix
#  $DIR        = current file directory
